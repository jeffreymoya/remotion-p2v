{
  "defaultProvider": "gemini-cli",
  "language": "en",
  "providers": {
    "codex": {
      "name": "codex",
      "cliCommand": "codex",
      "defaultModel": "gpt-4-turbo-preview",
      "temperature": 0.7,
      "maxTokens": 8000,
      "enabled": false
    },
    "claude-code": {
      "name": "claude-code",
      "cliCommand": "claude",
      "defaultModel": "claude-3-5-sonnet-20241022",
      "temperature": 0.7,
      "maxTokens": 4000,
      "enabled": false
    },
    "gemini-cli": {
      "name": "gemini-cli",
      "cliCommand": "gemini",
      "_comment": "Gemini CLI uses its own internal model config (e.g., gemini-2.5-flash-lite). No defaultModel needed.",
      "temperature": 0.7,
      "maxTokens": 8000,
      "enabled": true
    }
  },
  "fallbackOrder": [
    "gemini-cli"
  ]
}
